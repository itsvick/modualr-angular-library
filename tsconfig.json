{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@project-sunbird/common-consumption/cards": [
        "./dist/common-consumption/cards"
      ],
      "@project-sunbird/common-consumption/cards/*": [
        "./dist/common-consumption/cards/*"
      ],
      "layout": [
        "./dist/common-consumption/layout"
      ],
      "layout/*": [
        "./dist/common-consumption/layout/*"
      ],
      "library-filters": [
        "./dist/common-consumption/library-filters"
      ],
      "library-filters/*": [
        "./dist/common-consumption/library-filters/*"
      ],
      "modal": [
        "./dist/common-consumption/modal"
      ],
      "modal/*": [
        "./dist/common-consumption/modal/*"
      ]
    }
  }
}