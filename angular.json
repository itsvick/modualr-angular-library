{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cards": {
      "root": "projects/cards",
      "sourceRoot": "projects/cards/src",
      "projectType": "library",
      "prefix": "sb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/cards/tsconfig.lib.json",
            "project": "projects/cards/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/cards/src/test.ts",
            "tsConfig": "projects/cards/tsconfig.spec.json",
            "karmaConfig": "projects/cards/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/cards/tsconfig.lib.json",
              "projects/cards/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "layout": {
      "root": "projects/layout",
      "sourceRoot": "projects/layout/src",
      "projectType": "library",
      "prefix": "sb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/layout/tsconfig.lib.json",
            "project": "projects/layout/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/layout/src/test.ts",
            "tsConfig": "projects/layout/tsconfig.spec.json",
            "karmaConfig": "projects/layout/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/layout/tsconfig.lib.json",
              "projects/layout/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "libraryFilters": {
      "root": "projects/library-filters",
      "sourceRoot": "projects/library-filters/src",
      "projectType": "library",
      "prefix": "sb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/library-filters/tsconfig.lib.json",
            "project": "projects/library-filters/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/library-filters/src/test.ts",
            "tsConfig": "projects/library-filters/tsconfig.spec.json",
            "karmaConfig": "projects/library-filters/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/library-filters/tsconfig.lib.json",
              "projects/library-filters/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "modal": {
      "root": "projects/modal",
      "sourceRoot": "projects/modal/src",
      "projectType": "library",
      "prefix": "sb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/modal/tsconfig.lib.json",
            "project": "projects/modal/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/modal/src/test.ts",
            "tsConfig": "projects/modal/tsconfig.spec.json",
            "karmaConfig": "projects/modal/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/modal/tsconfig.lib.json",
              "projects/modal/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "cards"
}